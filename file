+main:
+  li x1, 0x5
+  li x2, 0x3
+  li x3, 0x3
+  li x4, 0x3
+  nop
+  nop
+  nop
+  nop
+  nop
+  nop
+label:
+  j label
+.data
+#test data goes here
+tdat0:  .word 0x00000080
