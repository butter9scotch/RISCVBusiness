-ifneq (,$(findstring $(GRIDHOSTS), $(HOST)))
-%:
-	@$(if \
-		$(findstring $@, $(word 1, $(MAKECMDGOALS))), \
-		grid $(MAKE) $(MAKECMDGOALS) -$(MAKEFLAGS), \
-		echo "do nothing" > /dev/null)
