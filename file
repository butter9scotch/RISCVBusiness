-      		mod = 4'b1000;
-	end else if ((shifted_amount == 0) & (same_compare == 1'b0)) begin
-		temp_fraction = sol_frac - round_amount;
-		temp_exp = exp_in;
-      		mod = 4'b1001;
-	end else if ((fp1[31] == 1'b1) & (fp2[31] == 1'b0) & (cmp_out == 1'b1) & (sign == 1'b1)) begin
