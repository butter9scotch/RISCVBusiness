#! /usr/bin/env python
#encoding: utf-8

# Add your extension as a directory to recurse into here

def configure(cnf):
  cnf.recurse(template)
  cnf.recurse(rv32m)
  cnf.recurse(crc32)
  cnf.recurse(fpu)

def sim_source(cnf):
  cnf.recurse(template)
  cnf.recurse(rv32m)
  cnf.recurse(crc32)
  cnf.recurse(fpu)
